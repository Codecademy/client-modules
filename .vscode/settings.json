{
  "eslint.options": {
    "extensions": [".js", ".jsx", ".md", ".ts", ".tsx"]
  },
  "eslint.probe": [
    "markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html"
  ],
  "eslint.rules.customizations": [{ "rule": "*", "severity": "downgrade" }],
  "eslint.validate": [
    "markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html"
  ],
  "search.exclude": {
    "docs": true
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}
